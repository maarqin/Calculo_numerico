		float[][] valores = {
				{1, 2, 3, 4, 10}, 
				{2, 1, 2, 3, 7},
				{3, 2, 1, 2, 6}, 
				{4, 3, 2, 1, 5}
		};
		
		int m = 0;
		while( m <= 4 ) {
			for (int i = 1; i < valores.length; i++) {
				float multiplicador = valores[m][m] / valores[m][m];
	
				for (int j = 0; j < valores[i].length; j++) {
					float r = valores[i][j] - (multiplicador * valores[i-1][j]);
					
					valores[i][j] = r;
				}
			}
			
			m++;
		}
		
		
